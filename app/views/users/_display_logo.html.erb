<% if experience[:title] == "Freelance" %>
  <img class="avatar avatar-mini-logo" src="<%= letter_avatar_url("Freelance".downcase, 200) %>">
<% else %>
  <img class="avatar avatar-mini-logo" src="//logo.clearbit.com/<%= experience[:company].downcase.delete(' ') %>.com" onerror="this.src='<%= letter_avatar_url(experience[:company].downcase, 200) %>'">
<% end %>
